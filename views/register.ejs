<body>

	<div class="page-loading">
		<img src="images/loader.gif" alt="" />
		<span>Skip Loader</span>
	</div>

	<div class="theme-layout" id="scrollup">
		<section>
			<div class="remove-bottom">
				<div class="container">
					<div class="row">
						<div class="col-lg-12">
							<div id="account-popup-area" class="account-popup-area signup-popup-box static">
								<div class="account-popup custom-account-popup">
									<div class="background-register">
										<img src="images/jurisgo_aplat_crop.png"/>
										<div class="select-user">
												<span class="register-choose-candidate"
												id="employer_create_account">Recruteur</span>
											<span class="register-choose-recruiter"
												id="candidate_create_account">Candidat</span>
										</div>
									</div>
									<form>
										<div class="cfield">
											<input id="lastname_create_account" type="text" placeholder="Nom" />
											<i class="la la-user"></i>
										</div>
										<div class="cfield">
											<input id="firstname_create_account" type="text" placeholder="Prénom" />
											<i class="la la-user"></i>
										</div>	
										<div class="cfield">
											<input id="phone_create_account" type="text" placeholder="Téléphone" />
											<i class="la la-phone"></i>
										</div>
										<div class="cfield">
											<input id="mail_create_account" type="text" placeholder="Email" />
											<i class="la la-envelope-o"></i>
										</div>
										<div class="cfield">
											<input id="password_create_account" type="password"
												placeholder="Mot de passe" />
												<i id="see-password-account" class="la la-eye-slash" style="cursor:pointer;"></i>
											</div>
										<div class="cfield">
											<input id="confirm_password_create_account" type="password"
												placeholder="Confirmez le mot de passe" />
												<i id="see-confirm-password-account" class="la la-eye-slash" style="cursor:pointer;"></i>
											</div>
										<div class="dropdown-field">
											<select id="genre_create_account" data-placeholder="Genre" class="chosen">
												<option>Homme</option>
												<option>Femme</option>
												<option>Autre</option>
											</select>
										</div>
										<button id="button_create_account" type="submit">Créer un compte</button>
										<button id="button_register_to_connexion" type="submit">Connectez-vous</button>
									</form>

								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</section>

