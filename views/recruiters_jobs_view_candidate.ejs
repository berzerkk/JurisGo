<!DOCTYPE html>
<html>

<head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<title>Jurisgo</title>
	<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
	<meta name="description" content="">
	<meta name="keywords" content="">
	<meta name="author" content="CreativeLayers">

	<!-- Styles -->
	<link rel="stylesheet" type="text/css" href="stylesheets/bootstrap-grid.css" />
	<link rel="stylesheet" href="stylesheets/icons.css">
	<link rel="stylesheet" href="stylesheets/animate.min.css">
	<link rel="stylesheet" type="text/css" href="stylesheets/style.css" />
	<link rel="stylesheet" type="text/css" href="stylesheets/responsive.css" />
	<link rel="stylesheet" type="text/css" href="stylesheets/chosen.css" />
	<link rel="stylesheet" type="text/css" href="stylesheets/colors/colors.css" />
	<link rel="stylesheet" type="text/css" href="stylesheets/bootstrap.css" />
	<link rel="stylesheet" type="text/css" href="stylesheets/bootstrap-datetimepicker.min.css" />
	<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css" integrity="sha384-oS3vJWv+0UjzBfQzYUhtDYW+Pj2yciDJxpsK1OYPAYjqT085Qq/1cq5FLXAZQ7Ay"
	 crossorigin="anonymous">

</head>

<body>

	<div class="page-loading">
		<img src="images/loader.gif" alt="" />
		<span>Skip Loader</span>
	</div>

	<div class="theme-layout" id="scrollup">

		<%- include header.ejs -%>

		<section class="overlape">
			<div class="block no-padding">
				<div data-velocity="-.1" style="background: url(http://placehold.it/1600x800) repeat scroll 50% 422.28px transparent;"
				 class="parallax scrolly-invisible no-parallax"></div><!-- PARALLAX BACKGROUND IMAGE -->
				<div class="container fluid">
					<div class="row">
						<div class="col-lg-12">
							<div class="inner-header">
								<div class="container">
									<div class="col-lg-12">
										<div class="action-inner"></div>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</section>

		<section class="overlape">
			<div class="block remove-top">
				<div class="container">
					<div class="row">
						<div class="col-lg-12">
							<div class="cand-single-user">
								<div class="share-bar circle"></div>
								<div id="jobs-view-candidate-profil" class="can-detail-s">
								</div>
								<i class="heart far fa-heart"></i>
								<i class="evaluate fas fa-award"></i>
							</div>
							<!-- <ul class="cand-extralink">
				 			<li><a id="education-menu" href="#education">Education</a></li>
				 			<li><a id="experience-menu" href="#experience">Experience</a></li>
				 			<li><a id="skills-menu" href="#skills">Skills</a></li>
				 		</ul> -->
							<div class="cand-details-sec">
								<div class="row">
									<div class="col-lg-8 column">
										<div class="cand-details" id="about">
											<div class="edu-history-sec" id="education"></div>
											<div class="edu-history-sec" id="experience"></div>
											<div class="progress-sec" id="skills"></div>
										</div>
									</div>
									<div class="col-lg-4 column">
										<div class=quick-form-job>
											<h3>Programmer un rendez-vous</h3>
											<form>
												<input id="job-candidate-date-appointment" type="date" />
												<input id="job-candidate-hour-appointment" type="time" />
												<input id="job-candidate-address-appointment" type="text" />
												<button id="job-candidate-appointment" class="submit">Verrouiller</button>
											</form>

										</div>
									<div class="quick-form-job">
										<h3>Contacter par mail</h3>
										<form>
											<textarea id="job-candidate-message" placeholder="Tapez votre message ici."></textarea>
											<button id="job-candidate-send-message" class="submit">Envoyer</button>
										</form>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
	</div>
	</section>

	<%- include bottom_footer.ejs -%>


	</div>

	<%- include sidebar.ejs -%>

	<script src="javascripts/jquery.min.js" type="text/javascript"></script>
	<script src="javascripts/modernizr.js" type="text/javascript"></script>
	<script src="javascripts/script.js" type="text/javascript"></script>
	<script src="javascripts/bootstrap.min.js" type="text/javascript"></script>
	<script src="javascripts/bootstrap-datetimepicker.min.js" type="text/javascript"></script>
	<script src="javascripts/wow.min.js" type="text/javascript"></script>
	<script src="javascripts/slick.min.js" type="text/javascript"></script>
	<script src="javascripts/parallax.js" type="text/javascript"></script>
	<script src="javascripts/select-chosen.js" type="text/javascript"></script>
	<script src="https://code.jquery.com/ui/1.12.1/jquery-ui.min.js" type="text/javascript"></script>
	<script src="javascripts/recruiter_jobs_view_candidate.js" type="text/javascript"></script>

</body>

</html>